    <ElectronicText dataResourceId="HELP_GETSTART" createdStamp="2009-09-25 00:00:00.000">
        <textData><![CDATA[
Welcome to Product From Thailand! We are glad you are here. We will help you learn how to get started and how to get help when you need it.
        ]]></textData>
    </ElectronicText>
    <Content contentId="HELP_GETSTART" contentTypeId="DOCUMENT" ownerContentId="HELPROOT" dataResourceId="HELP_GETSTART" statusId="CTNT_PUBLISHED" privilegeEnumId="CNT_PRV_BRONZE" contentName="Getting started" description="Getting started on Product From Thailand" createdDate="2009-09-25 00:00:00.000"/>
