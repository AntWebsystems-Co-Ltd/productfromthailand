    <DataResource dataResourceId="HELP_SIGNIN" dataResourceTypeId="ELECTRONIC_TEXT" dataTemplateTypeId="NONE" statusId="CTNT_PUBLISHED" createdDate="2009-09-25 00:00:00.000"/>
    <ElectronicText dataResourceId="HELP_SIGNIN" createdStamp="2009-09-25 00:00:00.000">
        <textData><![CDATA[
            <div class="head2">Registration steps</div>
            <div class="tabletext">To register, click the 'Sign in or Register' link at the top right of most Product From Thailand pages , and follow these steps:</div>
            <div class="tabletext">
                <ol>
                    <li>Click the 'Create' link below 'Login' button.</li>
                    <li>Enter your contact and personal information.</li>
                    <li>Enter your email address.</li>
                    <li>Choose a username, password, and password hint.<br/>
                    <li>Click 'Save' to complete the registration process, otherwise click 'Back' to go to the previous page.</li>
                </ol>
            </div>
        ]]></textData>
    </ElectronicText>
    <Content contentId="HELP_SIGNIN" contentTypeId="DOCUMENT" ownerContentId="HELPROOT" dataResourceId="HELP_SIGNIN" statusId="CTNT_PUBLISHED" privilegeEnumId="CNT_PRV_BRONZE" contentName="Signin&amp;Register" description="Signing in &amp; Registration" createdDate="2009-09-25 00:00:00.000"/>
    <ContentAssoc contentId="HELPROOT" contentIdTo="HELP_SIGNIN" contentAssocTypeId="SUB_CONTENT" sequenceNum="2" fromDate="2009-09-25 00:00:00.000" createdDate="2009-09-25 00:00:00.000" />
    
    <DataResource dataResourceId="HELP_EDITACCOUNT" dataResourceTypeId="ELECTRONIC_TEXT" dataTemplateTypeId="NONE" statusId="CTNT_PUBLISHED" createdDate="2009-09-25 00:00:00.000" />
    <ElectronicText dataResourceId="HELP_EDITACCOUNT" createdStamp="2009-09-25 00:00:00.000">
        <textData><![CDATA[
            <div class="head2">To update your account information:</div>
            <div class="tabletext">
                <ol>
                    <li>Click your name at the top of most Product From Thailand pages.</li>
                    <li>click the link that corresponds to the account information you want to update.</li>
                    <li>Enter your account information you want to update and click 'Save' button to complete the process.</li>
                </ol>
            </div>
        ]]></textData>
    </ElectronicText>
    <Content contentId="HELP_EDITACCOUNT" contentTypeId="DOCUMENT" ownerContentId="HELPROOT" dataResourceId="HELP_EDITACCOUNT" statusId="CTNT_PUBLISHED" privilegeEnumId="CNT_PRV_BRONZE" contentName="Edit your account" description="How to edit your account?" createdDate="2009-09-25 00:00:00.000"/>
