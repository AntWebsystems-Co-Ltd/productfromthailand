    <DataResource dataResourceId="HELP_SEARCH" dataResourceTypeId="ELECTRONIC_TEXT" dataTemplateTypeId="NONE" statusId="CTNT_PUBLISHED" createdDate="2009-09-25 00:00:00.000"/>
    <ElectronicText dataResourceId="HELP_SEARCH" createdStamp="2009-09-25 00:00:00.000">
        <textData><![CDATA[
            <div class="head2">Search products steps</div>
            <div class="tabletext">To search products, go to the search section on the top of most Product From Thailand page, and follow these steps:</div>
            <div class="tabletext">
                <ol>
                    <li>Select the categories that you want to search products </li>
                    <li>You need to enter keyword or product name that you want to search and click 'Find' button.</li>
                </ol>
            </div>
        ]]></textData>
    </ElectronicText>
